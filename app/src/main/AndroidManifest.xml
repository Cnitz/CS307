<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cs307.com.playdeck" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="sensorLandscape" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".feedBackPage"
            android:label="@string/title_activity_feed_back_page" >
        </activity>
        <activity
            android:name=".findGamePage"
            android:label="@string/title_activity_find_game_page" >
        </activity>
        <activity
            android:name=".gamePage"
            android:label="@string/title_activity_game_page" >
        </activity>
    </application>

</manifest>
